{
    "name": "evergram-common",
    "description": "Evergram's common functionality",
    "keywords": [
        "express",
        "mongoose",
        "mongodb",
        "passport"
    ],
    "version": "0.0.1",
    "private": true,
    "author": "Josh Stuart",
    "repository": {
        "type": "git",
        "url": "https://github.com/evergram/evergram-common.git"
    },
    "engines": {
