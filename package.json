        "cookie-session": "1.0.2",
        "express": "4.9.0",
        "express-session": "1.8.2",
        "method-override": "2.2.0",
        "mongoose": "3.8.16",
        "passport": "0.2.1",
        "passport-instagram": "0.1.2"
    },
    "devDependencies": {
        "supertest": "0.13.0",
        "should": "4.0.4",
        "mocha": "1.21.4",
        "nodemon": "1.2.1"
